{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Dev Server - Start Casablanca-Settat Ju-Jitsu League Platform",
			"type": "shell",
			"command": "npm",
			"args": [
				"run",
				"dev"
			],
			"group": "build",
			"isBackground": true,
			"problemMatcher": [
				"$tsc"
			],
			"presentation": {
				"echo": true,
				"reveal": "always",
				"focus": false,
				"panel": "new"
			},
			"options": {
				"cwd": "${workspaceFolder}"
			}
		}
	]
}